{
  "name": "@launchpadlab/lp-redux-utils",
  "version": "1.1.0",
  "description": "redux helpers",
  "main": "lib/index.js",
  "scripts": {
    "start": "yarn install && yarn run build:development",
    "build": "mkdir -p lib && babel src --out-dir lib --no-comments --minified",
    "build:development": "mkdir -p lib && babel src --watch --out-dir lib",
    "clean": "rimraf lib",
    "docs": "documentation build src/index.js -f md -o docs.md",
    "lint": "eslint src",
    "precommit": "yarn run docs && git add docs.md",
    "prepublish": "yarn run lint && yarn run clean && yarn run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": "launchpadlab/lp-redux-utils",
  "homepage": "https://github.com/launchpadlab/lp-redux-utils",
  "keywords": [
    "redux",
    "utils"
  ],
  "author": {
    "name": "David Pickart",
    "email": "david@launchpadlab.com"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib/"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "codeclimate-test-reporter": "^0.4.1",
    "documentation": "^5.3.3",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "redux": "^3.0.0"
  }
}
